{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run Menuify",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "runtimeExecutable": "${workspaceRoot}\\menuify.cmd",
            "internalConsoleOptions": "neverOpen",
            "env": {
                "MENUIFY_MODE": "debug"
            }
        }
    ]
}